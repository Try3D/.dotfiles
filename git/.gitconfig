[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true

[user]
	name = Try3D
	email = 94473657+Try3D@users.noreply.github.com

[init]
	defaultBranch = master

[credential]
	helper = cache --timeout=86400

[core]
	editor = nvim
        pager = delta

[interactive]
        diffFilter = delta --color-only

[delta]
        navigate = true

[merge]
        conflictstyle = diff3

[diff]
        colorMoved = default
